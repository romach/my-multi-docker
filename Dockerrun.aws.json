{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "romach007/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "memory": 128,
      "links": ["client"]
    },
    {
      "name": "client",
      "image": "romach007/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    }
  ]
}